{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "rules": {
        "indent": "off",
        "@typescript-eslint/indent": ["warn", 4],
        "@typescript-eslint/space-before-function-paren": 0
    }
}
